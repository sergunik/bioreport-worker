APP_ENV=dev
LOG_LEVEL=INFO
DB_HOST=host.docker.internal
DB_PORT=5432
DB_DATABASE=bioreport
DB_USERNAME=bioreport
DB_PASSWORD=supersecretpassword
MAX_JOB_ATTEMPTS=3
JOB_POLL_INTERVAL_SECONDS=5
PDF_ENGINE=pdfplumber

# Normalization provider: openai | example | openrouter | groq | together | deepseek | ollama | openai_compatible
NORMALIZATION_PROVIDER=openai

# OpenAI (when NORMALIZATION_PROVIDER=openai)
NORMALIZATION_OPENAI_API_KEY=
NORMALIZATION_OPENAI_MODEL_NAME=gpt-4.1-mini
NORMALIZATION_OPENAI_TIMEOUT_SECONDS=30
NORMALIZATION_OPENAI_RATE_LIMIT_PER_MINUTE=60
